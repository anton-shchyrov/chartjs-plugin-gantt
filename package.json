  "homepage": "https://github.com/anton-shchyrov/chartjs-plugin-gantt#readme",
  "devDependencies": {
    "browserify": "^16.2.3",
    "browserify-shim": "^3.8.14",
    "esmify": "^2.1.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.7",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify-es": "^1.0.4",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "chart.js": "^2.8.0"
